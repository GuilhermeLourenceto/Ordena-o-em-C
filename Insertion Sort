void insertionSort( int vetor[], int tamanho_vetor ) {
    for ( int i = 1 ; i < tamanho_vetor ; i++ ) {
        int valor_atual = vetor[i];
        int posicao = i - 1;

        while ( posicao >= 0 && vetor[posicao] > valor_atual ) {
            vetor[posicao + 1] = vetor[posicao];
            posicao--;
        }

        vetor[posicao + 1] = valor_atual;
    }
}
