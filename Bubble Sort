void bubbleSort( int vetor[], int tamanho_vetor ) {
    for ( int i = 0 ; i < tamanho_vetor - 1 ; i++ )
        for ( int j = 0 ; j < tamanho_vetor - i - 1 ; j++ )
            if ( vetor[j] > vetor[j + 1] ) {
                int temporario = vetor[j];
                vetor[j] = vetor[j + 1];
                vetor[j + 1] = temporario;
            }
}
